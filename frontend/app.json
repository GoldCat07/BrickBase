{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "frontend",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.realestate.inventory",
      "config": {
        "googleMapsApiKey": "AIzaSyC46BsGdP0YtuAuxdlgP8rTni1vwmy4oDA"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Take property photos",
        "NSPhotoLibraryUsageDescription": "Select property photos",
        "NSLocationWhenInUseUsageDescription": "Tag photos with location",
        "NSLocationAlwaysUsageDescription": "Tag photos with location"
      }
    },
    "android": {
      "package": "com.realestate.inventory",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC46BsGdP0YtuAuxdlgP8rTni1vwmy4oDA"
        }
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to access your location to tag property photos with location data.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to access your location to tag property photos with location data."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take property photos.",
          "microphonePermission": false
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to upload property images."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
